{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "quotes": ["error", "double"],
      "semi": ["error", "always"]
      "require-jsdoc" : 0
    }
}
